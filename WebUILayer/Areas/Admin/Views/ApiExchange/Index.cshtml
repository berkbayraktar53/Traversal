@model IEnumerable<BookingExchangeViewModel.Exchange_Rates>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h1>Booking Api Kur Bilgileri</h1>
<br />
<table class="table table-striped">
    <tr>
        <th>Kur Adı</th>
        <th>Kur Oranı</th>
    </tr>
    @foreach (var exchange in Model)

    {
        <tr>
            <td>@exchange.Currency</td>
            <td>@exchange.Exchange_rate_buy</td>
        </tr>
    }
</table>